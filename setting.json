{
  "name-web":"nbcnews",
  "author":"gakyang",
  "remove-comment-html":true,
  "target":"https://www.nbcnews.com",
  "name-folder-sitemap":"sitemap",
  "element-remove":[
    "script",
    "noscript",
    "meta",
    "iframe",
    "embed",
    "ins",
    "form",
    ".js-consent-banner",
    ".js-dismissable-hero",
    ".js-sidebar-zone",
    "#teams-promo",
    ".quickedit"
  ],
  "costom-element-remove":[
    {
      "target":"nbcnews.com",
      "element-remove-selector":[
        ".js-dismissable-hero",
        ".lazy-loading"
      ],
      "replace-string":[
        {
          "target":"https://www.nbcnews.com",
          "replace":"${nameWeb}"
        }
      ]
    }
  ],
  "inject-element-head":[
	{
      "name-element":"meta",
      "data-attribute":[
        {
          "name-attribute":"name",
          "value-attribute":"google-site-verification"
